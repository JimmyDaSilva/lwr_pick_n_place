<launch>

  <include file="$(find lwr_utils)/launch/run.launch">
    <arg name="ops_script" value="$(find rtt_lwr_sim)/scripts/run.ops"/>
    <arg name="sim" value="true"/>
    <arg name="log_level" value="info"/>
    <arg name="gazebo_gui" value="false"/>
    <arg name="load_base" value="true"/>
    <arg name="load_ati_sensor" value="true"/>
    <arg name="load_handle" value="true"/>
    <arg name="load_calib_tool" default="true"/>
  </include>

</launch>

